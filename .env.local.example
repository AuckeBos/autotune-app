# Autotune App - Local Development Environment Variables
# Copy this file to .env.local and update with your values

# Prefect Configuration
PREFECT_API_URL=http://prefect-server:4200/api
PREFECT_HOME=/app/.prefect

# Nightscout Configuration
NIGHTSCOUT_URL=https://your-site.herokuapp.com
NIGHTSCOUT_API_SECRET=your-api-secret-here

# Application Configuration
LOG_LEVEL=DEBUG
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1

# Optional: Streamlit Configuration (for web UI)
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0
